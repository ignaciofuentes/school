
<h2>Add new relationship for <%=@student.full_name%>
</h2>
  <%= form_for [@student, Relationship.new], :html => {:id=> "modal-form"} do |f| %>

  <div id="form-errors"></div>

   <p>
	<label for="name"> Name</label>
	<%= f.collection_select :relationship_type_id, RelationshipType.all, :id, :name, {:class=>"select"}%>
</p>
<%= f.hidden_field :guardian_id%>


<input type="submit"  class="hidden" />

  <% end %>

