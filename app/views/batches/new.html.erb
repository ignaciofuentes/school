<div class="container_12">
	<div class="grid_1">
		<%= image_tag "menu_icons/courses.png" %>
	</div>
	<div class="grid_9">
		<h1><%=link_to "Courses", courses_path%> &raquo; New</h1>
	</div>
	<%= render :partial => 'shared/back_button' %>	
</div>
<div class="clearfix">&nbsp;</div>


<div class="container_12">

	<% unless flash[:notice].nil? %>
	<p class="flash-msg"> <%= flash[:notice] %> </p>
	<% end %>


	<div class="bread_crumb">
		<%= link_to 'Courses', courses_path %> &raquo;
		<%= link_to @course.full_name, @course %> &raquo;
		New batch
	</div>

	<%= render "batch_form", :form_name=>"New Batch"%>
	

</div>
