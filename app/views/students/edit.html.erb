<% content_for :head do %>
<script>
	$(function() {
		$( "#tabs" ).tabs();
	});
	</script>
<%end%>

<div class="container_12">
	<div class="grid_1">
		<%= image_tag("menu_icons/admission.png") %>
	</div>
	<div class="grid_9">
			<h1><%= t "students.student_admission" %></h1>
	</div>
	<div class="grid_2 icon_button">
		<%= link_to :back do %>
		<%= image_tag("/images/buttons/back.png")%>
		<span><%= t"general.back" %></span>
		<% end %>
	</div>
</div>

<div class="clearfix">&nbsp;</div>

<%= form_for @student, :html=> {:multipart=>true} do |f| %>

<%= render 'shared/error_messages', :target => @student %>
<%= render "student_fields",  :f => f %>



<div class="clearfix">&nbsp;</div>
<div class ="container_12">	
		<%= f.submit "submit", :class=>"submit" %>
</div>

<% end %>

